    "@grafana/ui": "^7.0.0",
    "@types/lodash": "latest"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "dependencies": {
    "patch-package": "^6.2.2",
    "react-intl-universal": "^2.4.1"
